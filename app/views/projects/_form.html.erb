<%= form_with(model: @project, local: true) do |form| %>
  <div class="col-sm-4">
    <%= form.label :name, { class: "form-label"} %>
    <%= form.text_field :name, { class: "form-control", placeholder: "Project Name" } %>
  </div>
  <div class="col-sm-4">
    <%= form.submit "Create", class: "btn btn-primary GetWeatherBTN" %>
  </div>
<% end %>
